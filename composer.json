{
    "name": "edalzell/blade-directives",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Edalzell\\Blade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Edalzell\\Blade\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "statamic/cms": "3.0.*@beta",
        "php": "^7.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^3.0 || ^4.1",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "orchestra/testbench": "^4.0 || ^5.0"
    },
    "description": "Blade directives that work w/ Statamic",
    "authors": [
        {
            "name": "Erin Dalzell",
            "email": "erin@silentz.co",
            "homepage": "https://silentz.co",
            "role": "Founder"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Blade Directives",
            "description": "Blade Directives addon"
        },
        "laravel": {
            "providers": [
                "Edalzell\\Blade\\ServiceProvider"
            ]
        }
    }
}
