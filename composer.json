{
    "name": "edalzell/blade-directives",
    "license": "MIT",
    "description": "Blade directives that work w/ Statamic",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "statamic/cms": "^5.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.1 || ^7.0 || ^8.0",
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Edalzell\\Blade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Edalzell\\Blade\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Erin Dalzell",
            "email": "erin@silentz.co",
            "homepage": "https://silentz.co",
            "role": "Founder"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Blade Directives",
            "description": "Blade Directives addon"
        },
        "laravel": {
            "providers": [
                "Edalzell\\Blade\\ServiceProvider"
            ]
        }
    }
}
